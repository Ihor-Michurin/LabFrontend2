@page
@model EditModel
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<form method="post">
    <input type="hidden" asp-for="Analysis.Id" />

    <div class="form-group">
        <label asp-for="Analysis.Duration"></label>
        <input asp-for="Analysis.Duration" class="form-control" />
        <span asp-validation-for="Analysis.Duration" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Analysis.Name"></label>
        <input asp-for="Analysis.Name" class="form-control" />
        <span asp-validation-for="Analysis.Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Analysis.Price"></label>
        <input asp-for="Analysis.Price" class="form-control" />
        <span asp-validation-for="Analysis.Price" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Analysis.Description"></label>
        <input asp-for="Analysis.Description" class="form-control" />
        <span asp-validation-for="Analysis.Description" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Analysis.AnalysisValues"></label>
        <input asp-for="Analysis.AnalysisValues" class="form-control" />
        <span asp-validation-for="Analysis.AnalysisValues" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
